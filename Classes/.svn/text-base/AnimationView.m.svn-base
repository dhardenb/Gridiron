		NSMutableArray *frameArray = [self getFrameArray:player.name];
		
		for (NSMutableArray *array in frameArray)
		{
			CGPathAddLineToPoint(playerPath, NULL, xPos+[[array objectAtIndex:0] intValue], yPos+[[array objectAtIndex:1] intValue]);
