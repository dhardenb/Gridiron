- (NSMutableArray*)getFrameArray:(NSString *)position
{
	NSMutableArray *frameArray = [[NSMutableArray alloc] init];
	
	if ([GameManager getOffensivePositionSelected] == QUARTER_BACK)
	{
		if ([GameManager getAttackZoneSelected] == LEFT_INSIDE_RUSHING_LANE)
		{
			if ([GameManager getBasicPlayResult] == BAD_PLAY_RESULT)
			{
				frameArray = [self getFrameArrayForQuarterbackToInsideRushingLaneBad: frameArray :position];
			}
			else
			{
				frameArray = [self getFrameArrayForQuarterbackToInsideRushingLaneGood: frameArray :position];
			}
		}
	}
	else if ([GameManager getOffensivePositionSelected] == TAIL_BACK)
	{
		if ([GameManager getAttackZoneSelected] == LEFT_OFFTACKLE_RUSHING_LANE)
		{
			frameArray = [self getFrameArrayForTailbackToLeftOffTackleRushingLane: frameArray :position];
		}
		else if ([GameManager getAttackZoneSelected] == LEFT_INSIDE_RUSHING_LANE)
		{
			frameArray = [self getFrameArrayForTailbackToInsideRushingLane: frameArray :position];
		}
		else if ([GameManager getAttackZoneSelected] == RIGHT_OFFTACKLE_RUSHING_LANE)
		{
			frameArray = [self getFrameArrayForTailbackToRightOffTackleRushingLane: frameArray :position];
		}
	}
	
	return [self fuzzFrameArray:frameArray];
}

- (NSMutableArray*)fuzzFrameArray:(NSMutableArray*)frameArray
{
	int amountOfVariance = 50;
	int amountOfFuzz = 3;
	
	int rollForX, rollForY, x, y;
	
	NSMutableArray *array;
	
	for (int z = 2; z < [frameArray count]; z++)
	{
		array = [frameArray objectAtIndex:z];
		
		rollForX = [RollManager rollOneHundredSidedDice];
		rollForY = [RollManager rollOneHundredSidedDice];
		
		x = [[array objectAtIndex:0] intValue];
		
		if (rollForX < amountOfVariance)
		{
			x = x - amountOfFuzz;
		}
		else if (rollForX > 100 - amountOfVariance)
		{
			x = x + amountOfFuzz;
		}
		
		[array replaceObjectAtIndex:0 withObject:[NSNumber numberWithInt:x]];
		
		y = [[array objectAtIndex:1] intValue];
		
		if (rollForY < amountOfVariance)
		{
			y = y - amountOfFuzz;
		}
		else if (rollForX > 100 - amountOfVariance)
		{
			y = y + amountOfFuzz;
		}
		
		[array replaceObjectAtIndex:1 withObject:[NSNumber numberWithInt:y]];
	}
	
	return frameArray;
}

- (void)askQuestion:(int)promptButton
{	
	[(id<AskQuestions>)self.nextResponder askQuestion:promptButton];
}

- (void)hide
{
	if (isVisable)
	{
		CGContextRef context = UIGraphicsGetCurrentContext();
		
		self.alpha = 1.0;
		
		[UIView beginAnimations:nil context:context];
		[UIView setAnimationCurve:UIViewAnimationCurveLinear];
		[UIView setAnimationDuration:0.5];
		
		self.alpha = 0.0;
		
		[UIView commitAnimations];
		
		isVisable = FALSE;
	}
}

- (void)show
{
	if (!isVisable)
	{
		CGContextRef context = UIGraphicsGetCurrentContext();
		
		self.alpha = 0.0;
		
		[UIView beginAnimations:nil context:context];
		[UIView setAnimationCurve:UIViewAnimationCurveLinear];
		[UIView setAnimationDuration:0.5];
		
		self.alpha = 1.0;
		
		[UIView commitAnimations];
		
		isVisable = TRUE;
	}
}

- (BOOL)pointInside:(CGPoint)point withEvent:(UIEvent *)event 
{
	for (int x = 0; x < self.subviews.count; x++)
	{
		UIView *subview = [self.subviews objectAtIndex:x];
		
		if (point.x >= subview.frame.origin.x && point.x <= subview.frame.origin.x + [subview frame].size.width)
		{
			if (point.y >= subview.frame.origin.y && point.y <= subview.frame. origin.y + [subview frame].size.height)
			{
				return YES;
			}
		}
	}
	
	return NO;
}

